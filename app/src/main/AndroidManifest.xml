<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cristhianpinzon.lectorqr">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconoprincipal"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".UsuarioActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RedAcmPtsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginEmpleadosActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EmpleadoActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ContactanosActivity" />
        <activity android:name=".Welcome_activity"
            android:screenOrientation="locked"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Information_Activity"/>
    </application>

</manifest>